{
  "name": "@debridge-finance/dln-client",
  "description": "A client SDK that facilitates interaction deSwap Liquidity Network (DLN) contracts on Solana and EVM blockchains",
  "version": "17.6.2",
  "author": "deBridge",
  "license": "GPL-3.0-only",
  "homepage": "https://debridge.finance",
  "repository": "github:debridge-finance/dln-ts-client",
  "keywords": [
    "deBridge",
    "DLN",
    "sdk",
    "solana",
    "ethereum"
  ],
  "files": [
    "dist/**",
    "docs/**",
    "examples/**",
    "LICENSE",
    "readme.md"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "browser": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "lint": "eslint --config .eslintrc \"{src,tests}/**/*.{js,ts}\"",
    "lint:fix": "eslint --config .eslintrc \"{src,tests,examples}/**/*.{js,ts}\" --fix",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:node": "tsc -p tsconfig.cjs.json",
    "build:browser": "rollup -c",
    "build": "rimraf dist && npm run build:esm && npm run build:node",
    "test:utils": "mocha -r ts-node/register -b -t 20000000 ./tests/utils.test.ts",
    "test:state": "mocha -r ts-node/register -b -t 20000000 ./tests/common/state.test.ts",
    "test:patch": "mocha -r ts-node/register -b -t 2000000 ./tests/common/patch.test.ts",
    "test:solSrc": "mocha -r ts-node/register -b -t 20000000 ./tests/solana/src.test.ts",
    "test:solDst": "mocha -r ts-node/register -b -t 20000000 ./tests/solana/dst.test.ts",
    "test:sol": "mocha -r ts-node/register -b -t 20000000 ./tests/solana/client.test.ts",
    "test:evmSolBatch": "mocha -r ts-node/register -b -t 20000000 ./tests/common/send.from.evm.batchUnlock.in.solana.test.ts",
    "test:order": "mocha -r ts-node/register -b -t 20000000 ./tests/order.test.ts",
    "test:evm": "mocha -r ts-node/register -b -t 20000000 ./tests/evm/evm.test.ts",
    "test:common": "mocha -r ts-node/register -b -t 20000000 ./tests/common/new.client.test.ts",
    "test:newPreswap": "mocha -r ts-node/register -b -t 20000000 ./tests/common/new.client.preswap.test.ts",
    "test:evmSolSend": "mocha -r ts-node/register -b -t 20000000 ./tests/common/send.from.evm.fulfill.in.solana.test.ts",
    "test:solEvmSend": "mocha -r ts-node/register -b -t 20000000 ./tests/common/send.from.solana.fulfill.in.evm.test.ts",
    "test:evmEvmSend": "mocha -r ts-node/register -b -t 20000000 ./tests/common/send.from.evm.fulfill.in.evm.test.ts",
    "test:cancel": "mocha -r ts-node/register -b -t 20000000 ./tests/common/cancel.test.ts",
    "test:cost": "mocha -r ts-node/register -b -t 20000 ./tests/common/costs.test.ts",
    "test:evmFulfillCost": "mocha -r ts-node/register -b -t 20000 ./tests/evm/fulfill.cost.test.ts",
    "test:gaslimits": "mocha -r ts-node/register -b -t 20000 ./tests/evm/gaslimits.test.ts",
    "test:evmCalldata": "mocha -r ts-node/register -b -t 2000000 ./tests/calldata/evm.calldata.test.ts",
    "test:solanaCalldata": "mocha -r ts-node/register -b -t 2000000 ./tests/calldata/solana.calldata.test.ts",
    "test:calldataRewards": "mocha -r ts-node/register -b -t 2000000 ./tests/calldata/solana.calldata.rewards.test.ts",
    "test:fulfillPreswap": "mocha -r ts-node/register -b -t 20000000 ./tests/common/fulfill.preswap.test.ts",
    "test:createPreswap": "mocha -r ts-node/register -b -t 20000000 ./tests/common/create.preswap.test.ts",
    "test:solanaSwaps": "mocha -r ts-node/register -b -t 20000000 ./tests/solana/swaps.test.ts",
    "test:evmSwaps": "mocha -r ts-node/register -b -t 20000000 ./tests/evm/swaps.test.ts",
    "test:jupiter": "mocha -r ts-node/register -b -t 20000 ./tests/solana/jupiter.test.ts",
    "test:ci": "mocha -r ts-node/register -b -t 20000 --jobs 3 --file ./tests/solana/client.test.ts ./tests/evm/client.test.ts ./tests/order.test.ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "22.0.2",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-replace": "4.0.0",
    "@rollup/plugin-typescript": "8.5.0",
    "@types/bn.js": "5.1.1",
    "@types/chai": "4.3.3",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "9.1.1",
    "@types/node": "18.16.3",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "assert": "2.0.0",
    "chai": "4.3.6",
    "chai-as-promised": "^7.1.2",
    "dotenv": "16.0.3",
    "eslint": "8.44.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "^5.0.0-alpha.2",
    "mocha": "10.1.0",
    "mocha-profiler": "^1.0.0",
    "prettier": "3.0.0",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "typedoc": "0.24.8",
    "typedoc-plugin-markdown": "3.15.3",
    "typescript": "5.1.3",
    "undici": "^5.24.0"
  },
  "dependencies": {
    "@coral-xyz/anchor": "0.29.0",
    "@debridge-finance/debridge-external-call": "1.0.7",
    "@debridge-finance/solana-contracts-client": "4.1.3",
    "@debridge-finance/solana-transaction-parser": "2.0.1",
    "bignumber.js": "9.1.2",
    "buffer": "6.0.3",
    "js-sha3": "0.8.0",
    "node-cache": "5.1.2",
    "viem": "^2.7.3",
    "web3": "1.8.0",
    "web3-utils": "1.8.0"
  },
  "peerDependencies": {
    "@debridge-finance/solana-utils": "^5.0.0",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/buffer-layout-utils": "^0.2.0",
    "@solana/web3.js": "^1.89.1"
  }
}
